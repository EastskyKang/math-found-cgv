function [ output_args ] = PlotBestRansacResult( data, model_center, model_radius, tau,
    best_result_ransac, domain_size )
    % PLOTBESTRANSACRESULT
    
    % model_center          [x_c, y_c]
    % best_result_ransac    [n
    % domain_size           [x_min, x_max, y_min, y_max]
    
    % theta (central angle)
    theta = 0:0.01:pi()*2;
    
    % synthetic model 
    x_syntetic = radius * cos(theta) + model_center(1);
    y_syntetic = radius * sin(theta) + model_center(2);
    
    % ransac model
    ransac_circle_center = [best_result_ransac(3), best_result_ransac(4)];
    ransac_circle_radius = best_result_ransac(5);
    
    x_ransac = ransac_circle_radius * cos(theta) + ransac_circle_center(1);
    y_ransac = ransac_circle_radius * sin(theta) + ransac_circle_center(2);
    
    inlier_vectors = data(abs(pdist2(inlier_vectors, ransac_circle_center) - ransac_circle_radius) <= tau, :);
    outlier_vectors = data(abs(pdist2(inlier_vectors, ransac_circle_center) - ransac_circle_radius) > tau, :);
    
    % plotting 
    plot(x_syntetic, y_syntetic, 'g-')
    hold on
    plot(inlier_vectors(:, 1), inlier_vectors(:, 2), 'bo')
    plot(outlier_vectors(:, 1), outlier_vectors(:, 2), 'ro')
    plot(x_ransac, y_ransac, 'k-')
    hold off
    axis([x_min, x_max, y_min, y_max])
  
end

